{
  "name": "docpad-plugin-less",
  "version": "2.4.1",
  "description": "Adds support for the LESS CSS pre-processor to DocPad",
  "homepage": "http://docpad.org/plugin/less",
  "license": "MIT",
  "keywords": [
    "docpad",
    "docpad-plugin",
    "less",
    "less.js",
    "lesscss",
    "css",
    "styles",
    "styling"
  ],
  "badges": {
    "list": [
      "travisci",
      "npmversion",
      "npmdownloads",
      "daviddm",
      "daviddmdev",
